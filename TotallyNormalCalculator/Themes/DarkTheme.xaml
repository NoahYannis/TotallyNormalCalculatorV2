<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundColor" Color="#272B2F" />
    <SolidColorBrush x:Key="ForegroundColor" Color="White" />
    <SolidColorBrush x:Key="BorderBackground" Color="Indigo" />
    <SolidColorBrush x:Key="DiaryEntryColor" Color="Indigo" />
    <SolidColorBrush x:Key="MouseOver" Color="MediumPurple" />
    <SolidColorBrush x:Key="Selected" Color="Purple" />
    <SolidColorBrush x:Key="BorderBrush" Color="Blue" />
    <SolidColorBrush x:Key="RadioButtonMenu" Color="#272B2F" />
    <SolidColorBrush x:Key="BorderIndicator" Color="White" />
    <LinearGradientBrush x:Key="GradientBrush" StartPoint="0,1" EndPoint="1,0">
        <GradientStop Offset="0" Color="#272B2F" />
        <GradientStop Offset="1" Color="Indigo" />
    </LinearGradientBrush>

</ResourceDictionary>